#include <iostream>
/*
Ejemplo para uso de condicional if-else
*/
using namespace std;

int main()
{
    string respuesta="";
    cout<<" Para continuar escribe tu password"<<endl;
    cin>>respuesta;
    if(respuesta=="programa2021"){
        cout<<"Acceso concedido"<<endl;
        
    }
    else
    {
        cout<<"Acceso denegado"<<endl;
    }
    return 0;
}

